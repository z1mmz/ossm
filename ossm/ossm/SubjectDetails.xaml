<Window x:Class="ossm.SubjectDetails"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SubjectDetails" Height="339" Width="779">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="193*"/>
            <ColumnDefinition Width="385*"/>
            <ColumnDefinition Width="193*"/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="subjectDescBlock" Height="204" Margin="48,40,49,0" TextWrapping="Wrap" Text="subjectDesc" VerticalAlignment="Top" Grid.Column="1"/>
        <TextBlock x:Name="SubjectCodeBlock" HorizontalAlignment="Left" Height="53" Margin="30,53,0,0" TextWrapping="Wrap" Text="SubjectCode" VerticalAlignment="Top" Width="142"/>
        <TextBlock x:Name="subjectNameBlock" HorizontalAlignment="Left" Height="113" Margin="30,111,0,0" TextWrapping="Wrap" Text="subjectName" VerticalAlignment="Top" Width="142"/>

    </Grid>
</Window>
