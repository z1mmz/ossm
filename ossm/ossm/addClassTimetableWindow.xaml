<Window x:Class="ossm.addClassTimetableWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="addClassTimetableWindow" Height="300" Width="300">
    <Grid>
        <ComboBox x:Name="subjectCombo" HorizontalAlignment="Left" Margin="162,59,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="timeTextBox" HorizontalAlignment="Left" Height="23" Margin="162,179,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="dayCobo" HorizontalAlignment="Left" Margin="162,119,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Left" Margin="29,65,0,0" TextWrapping="Wrap" Text="Subject" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="29,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top"><Run Text="Day"/><LineBreak/><Run/></TextBlock>
        <TextBlock HorizontalAlignment="Left" Margin="29,186,0,0" TextWrapping="Wrap" Text="Time" VerticalAlignment="Top"/>
    </Grid>
</Window>
